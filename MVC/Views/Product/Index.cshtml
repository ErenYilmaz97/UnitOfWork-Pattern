@model List<Product>

    <div class="row mt-5">
        <div class="col-md-2"></div>
        <div class="col-md-8 text-center">



            @if (Model.Count == 0)
            {
                <div class="alert alert-danger mt-4">
                    <small>Ürün Bulunamadı.</small>
                </div>

            }
            else
            {
                <div class="mb-5" id="productTable">
                    @await Component.InvokeAsync("ProductTable",Model)
                </div>
            }

        </div>
        <div class="col-md-2"></div>
    </div>


